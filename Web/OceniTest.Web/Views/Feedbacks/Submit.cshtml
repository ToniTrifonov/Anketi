@model OceniTest.Web.ViewModels.Feedbacks.SubmitFeedbackInputModel

@{ 
    this.ViewData["Title"] = "Submit Feedback";
}

<h1>@this.ViewData["Title"]</h1>

<form method="post">
    <label asp-for="@Model.Comments"></label>
    <textarea asp-for="@Model.Comments"></textarea>
    <span asp-validation-for="@Model.Comments" class="alert-danger"></span>

    <input type="submit" class="btn btn-primary" asp-action="Submit" />
</form>
